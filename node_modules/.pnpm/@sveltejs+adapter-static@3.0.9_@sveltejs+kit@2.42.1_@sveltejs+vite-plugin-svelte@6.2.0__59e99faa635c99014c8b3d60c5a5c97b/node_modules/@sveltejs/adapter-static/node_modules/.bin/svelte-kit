#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/kit/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/kit/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules:/Users/ionutale/developer/demo-safari-address-bar-color-change/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../../../../@sveltejs+kit@2.42.1_@sveltejs+vite-plugin-svelte@6.2.0_svelte@5.38.10_vite@7.1.5_@type_e7bb383da7590d9819b9cd9ad0716f4d/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
